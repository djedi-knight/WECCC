{
  "name": "weccc",
  "version": "1.0.0",
  "description": "Outcome reporting tool for WECCC",
  "main": "index.js",
  "scripts": {
    "lint:js": "eslint -c .eslintrc src",
    "lint:jsx": "eslint -c .eslintrc --ext jsx src",
    "lint:json": "eslint -c .eslintrc --ext json src",
    "lint": "npm run lint:js & npm run lint:json & npm run lint:jsx",
    "build:dev": "webpack",
    "start:dev": "webpack-dev-server --progress --colors --display-error-details",
    "build:prod": "better-npm-run build",
    "test": "mocha --compilers js:babel-core/register --require node_modules/ignore-styles --require ./test/test_helper.js \"test/**/*@(.js|.jsx)\"",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djedi-knight/WECCC.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/djedi-knight/WECCC/issues"
  },
  "homepage": "https://github.com/djedi-knight/WECCC#readme",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.10",
    "chai": "^3.5.0",
    "chai-immutable": "^1.6.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.1",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "ignore-styles": "^4.0.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "normalize.css": "^4.2.0",
    "postcss-loader": "^0.9.1",
    "react-addons-css-transition-group": "^15.2.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "flexboxgrid": "^6.3.1",
    "immutable": "^3.8.1",
    "react": "^15.2.1",
    "react-addons-pure-render-mixin": "^15.2.1",
    "react-addons-update": "^15.3.1",
    "react-d3": "^0.4.0",
    "react-dom": "^15.2.1",
    "react-flexbox-grid": "^0.10.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.0.0",
    "react-toolbox": "^1.0.3",
    "react-tooltip": "^3.1.5",
    "redux": "^3.5.2"
  }
}
